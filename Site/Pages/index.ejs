<body>
     <!-- This is the Account Creationscreen and below are the Username, Password fields and the Login button -->
    <div class="field">
        <label for="username">Username:</label>
        <input type="text" id="username" name="username" required>
    </div>

    <div class="field">
        <label for="password">Password:</label>
        <input type="text" id="password" name="password" required>
    </div>
    <div class="field">
        <button type="submit" id="loginb">Login</button>
    </div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
<script src="/JS/main.js"></script>

<% if (locals.message) { %>
    <h2><%= locals.message %></h2>
<% } %>
<% if (locals.user_id) { %>
    <h2><%= locals.user_id %></h2>
<% } %> 
  <p>Organise your projects and organise your life</P>
</body>
